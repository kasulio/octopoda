{
  "name": "octopoda",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun --bun node_modules/.bin/next build",
    "check": "bun --bun node_modules/.bin/next lint && tsc --noEmit",
    "db:generate": "bun --bun node_modules/.bin/drizzle-kit generate",
    "db:migrate": "bun --bun node_modules/.bin/drizzle-kit migrate",
    "db:push": "bun --bun node_modules/.bin/drizzle-kit push",
    "db:studio": "bun --bun node_modules/.bin/drizzle-kit studio",
    "db:dbml": "bun drizzle/generate-dbml.ts",
    "dev": "bun --bun node_modules/.bin/next dev --turbo",
    "format:check": "bun --bun node_modules/.bin/prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "bun --bun node_modules/.bin/prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "bun --bun node_modules/.bin/next lint",
    "lint:fix": "bun --bun node_modules/.bin/next lint --fix",
    "preview": "bun --bun node_modules/.bin/next build && bun --bun node_modules/.bin/next start",
    "start": "bun --bun node_modules/.bin/next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.2",
    "@hookform/resolvers": "^3.9.1",
    "@libsql/client": "^0.14.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tanstack/react-query": "^5.62.7",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.38.0",
    "geist": "^1.3.1",
    "lucide-react": "^0.468.0",
    "next": "^15.1.0",
    "next-auth": "5.0.0-beta.25",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.2",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@types/bun": "^1.1.14",
    "@types/eslint": "^8.56.10",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "drizzle-dbml-generator": "^0.10.0",
    "drizzle-kit": "^0.30.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.1.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  }
}
